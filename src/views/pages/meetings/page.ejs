<section class="section">
  <div class="row">
    <div class="col-lg-12">
      <div class="card">
        <div class="card-body">
          <h5 class="card-title">Meetings</h5>
          <table id="myTable" class="hover row-border cell-border">
            <thead>
              <tr>
                <th>#</th>
                <th>Meeting Time</th>
                <th>Student</th>
                <th>Course</th>
              </tr>
            </thead>
            <tbody>
              <% data.forEach((d, i)=> { %>
                <tr>
                  <td>
                    <%= i + 1 %>
                  </td>
                  <td>
                    <% const options={ day: "2-digit" , month: "2-digit" , year: "numeric" , hour: "2-digit" ,
                      minute: "2-digit" , second: "2-digit" , hour12: false, timeZone: "Asia/Makassar" }%>
                      <%= d.date.toLocaleString(undefined, options)%>
                  </td>
                  <td>
                    <%= d.User.full_name %>
                  </td>
                  <td>
                    <%= d.meeting.name %>
                  </td>
                </tr>
                <% }) %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</section>