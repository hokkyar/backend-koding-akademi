<div class="pagetitle">
  <h1>
    <%= title %>
  </h1>
  <nav>
    <ol class="breadcrumb">
      <% if (sub==='' || sub==='Manage' || sub==='QR' ) { %>
        <li class="breadcrumb-item"><a href="/admin/dashboard">Home</a></li>
        <% }else{ %>
          <li class="breadcrumb-item"><a href="/admin/dashboard">Other</a></li>
          <% } %>
            <% if (sub==='Manage' ) { %>
              <li class="breadcrumb-item">Manage</li>
              <% } %>
                <li class="breadcrumb-item active">
                  <% if(page==='show-user' ){ %>
                    <a href="/admin/users">
                      <% }else if(page==='show-course' ){ %>
                        <a href="/admin/courses">
                          <% }else{ %>
                            <a href="/admin/<% page %>">
                              <% } %>
                                <%= title %>
                            </a>
                </li>
                <% if (detail ) { %>
                  <li class="breadcrumb-item">
                    <%= detail.id %>
                  </li>
                  <% } %>
    </ol>
  </nav>
</div>